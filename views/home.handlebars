<div class="container" xmlns:min-length="http://www.w3.org/1999/xhtml">
    <div class="form-container">
        <div class="row">
            <div class="col-md-12">
                <form class="user-info-form">
                    <div class="form-group">
                        <label for="name-input"> Name </label>
                        <input type="name" class="form-control" id="name-input" placeholder=
                                "name" required/>
                    </div>
                    <div class="form-group">
                        <label for="phone-number"> Phone </label>
                        <input type="number" class="form-control" id="phone-number" placeholder=
                                "phonenumber" required min-length:10/>
                    </div>

                    <button type="click" value="http://localhost:8080/customer" class="submit-user-info">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>
<script src="js/home.js" type="text/javascript></script>
